{
  "name": "ownstats",
  "description": "CLI for managing an OwnStats (https://ownstats.com) instance, providing a privacy-focused selfhosted web analytics solution on AWS",
  "version": "0.1.1",
  "author": "Tobias MÃ¼ller <tobilg@gmail.com>",
  "bin": {
    "ownstats": "./bin/run.js"
  },
  "bugs": "https://github.com/ownstats/cli/issues",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.716.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.723.0",
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/client-sts": "^3.716.0",
    "@aws-sdk/credential-providers": "^3.716.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "bunyan": "^1.8.15",
    "degit": "^2.8.4",
    "env-paths": "2.2.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.3.0",
    "nested-property": "^4.0.0",
    "terminal-link": "2.1.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/bunyan": "^1.8.11",
    "@types/chai": "^4",
    "@types/degit": "^2.8.6",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ownstats/cli",
  "keywords": [
    "ownstats",
    "cli",
    "webanalytics",
    "analytics",
    "metrics",
    "dashboard",
    "serverless",
    "aws",
    "selfhosted",
    "privacy"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "oclif": {
    "bin": "ownstats",
    "dirname": "ownstats",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Interact with OwnStats configuration"
      },
      "installation": {
        "description": "Create local OwnStats installations"
      },
      "stack": {
        "description": "Interact with OwnStats stacks within an installation"
      },
      "user": {
        "description": "Manage OwnStats users"
      },
      "verify": {
        "description": "Verify AWS credentials"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ownstats/cli.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
